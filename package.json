{
  "name": "status-n-price",
  "version": "0.1.1",
  "description": "Domain status + price quotes using domainstat and domain-quotes.",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:lib",
    "build:lib": "npx -y esbuild src/index.ts --bundle --minify --format=esm --target=es2022 --outdir=dist/ --platform=neutral --external:domainstat --external:domain-quotes",
    "test": "npm run build && node --test",
    "start": "npm run build && npx serve docs"
  },
  "homepage": "https://namewiz.github.io/status-n-price",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namewiz/status-n-price.git"
  },
  "bugs": {
    "url": "https://github.com/namewiz/status-n-price/issues"
  },
  "author": "Justice Ogbonna <dev@justiceo.com>",
  "keywords": [
    "domain-name",
    "domain-status",
    "domain-availability",
    "rdap",
    "whois"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "license": "MIT",
  "engines": {
    "node": "^22.12.0",
    "npm": "^10.9.0"
  },
  "dependencies": {
    "domain-quotes": "^0.0.20",
    "domainstat": "^1.0.6"
  },
  "devDependencies": {}
}
